{
    "name": "m1/twig-asset-rev-extension",
    "type": "library",
    "description": "Twig Asset Rev Extension adds a asset_rev twig filter so you can use asset revisioning from files, perfect for use with gulp-rev or grunt-rev",
    "keywords": [
        "M1",
        "twig-asset-rev-extension",
        "TwigAssetRevExtension",
        "twig",
        "asset",
        "rev",
        "extension",
        "gulp-rev",
        "grunt-rev",
        "asset_rev",
        "filter",
        "silex",
        "symfony",
        "css",
        "js",
        "javascript"
    ],
    "homepage": "https://github.com/M1/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": "Miles Croxford",
            "email": "hello@milescroxford.com",
            "homepage": "http://milescroxford.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "twig/twig": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "M1\\TwigAssetRevExtension\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M1\\TwigAssetRevExtension\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
